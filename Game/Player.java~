public class Player implements Character{

    private int hp;
    private int x;
    private int y;
    private int dx;
    private int dy;

    public Player(){
	hp = 100;
    }

    public int getX(){
	return x;
    }

    public int getY(){
	return y;
    }

    public void move(){
	x += dx;
	y += dy;
    }
}
